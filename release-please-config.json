{
  "packages": {
    ".": {
      "release-type": "node",
      "changelog-path": "CHANGELOG.md",
      "include-component-in-tag": false
    }
  },
  "branches": [
    {
      "branch": "main"
    },
    {
      "branch": "staging",
      "prerelease": true,
      "prerelease-type": "sta"
    },
    {
      "branch": "dev",
      "prerelease": true,
      "prerelease-type": "dev"
    }
  ],
  "separate-pull-requests": true,
  "commit-search-depth": 1000
}
